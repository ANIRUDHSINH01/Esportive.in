{
  "name": "esportive-in",
  "version": "1.0.0",
  "description": "Esportive.in - Express + EJS Tournament Platform with optional React frontend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build:frontend": "cd frontend && REACT_APP_BUILD_PATH=../build react-scripts build && cd ..",
    "frontend:dev": "cd frontend && react-scripts start && cd ..",
    "frontend:test": "cd frontend && react-scripts test && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "express": "^4.18.2",
    "express-rate-limit": "^6.10.0",
    "google-auth-library": "^10.3.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "proxy": "http://localhost:5000",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "esports",
    "tournaments",
    "express",
    "ejs",
    "react",
    "mongodb"
  ],
  "author": "Esportive Team",
  "license": "MIT"
}
